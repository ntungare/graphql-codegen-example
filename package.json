{
    "name": "graphql-codegen-example",
    "version": "1.0.0",
    "description": "Example project for GraphQL Code Generator",
    "keywords": [
        "graphql"
    ],
    "author": "Ninad Tungare",
    "email": "ninad.tungare@gmail.com",
    "license": "MIT",
    "packageManager": "pnpm@10.24.0",
    "scripts": {
        "pregraphql:codegen": "rm -rf src/generated/*",
        "graphql:codegen": "graphql-codegen --config codegen.ts",
        "codegen": "pnpm run pregraphql:codegen && pnpm run graphql:codegen"
    },
    "dependencies": {
        "@types/node": "^24.10.2",
        "graphql": "^16.12.0",
        "graphql-tag": "^2.12.6"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^6.1.0",
        "@graphql-codegen/client-preset": "^5.2.1",
        "@graphql-typed-document-node/core": "^3.2.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "5.9.3"
    }
}